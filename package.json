{
  "name": "notion-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn path && next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "path": "pathpida -o ./src/lib",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "test": "jest",
    "lint-staged": "yarn lint && yarn format",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storyshots": "NODE_ENV=test jest --config ./jest.storyshots.config.js",
    "update-storyshots": "NODE_ENV=test jest --config ./jest.storyshots.config.js --updateSnapshot"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@emotion/babel-plugin": "^11.7.2",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.16",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.3.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "postcss": "^8.4.6",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.5.1",
    "storybook": "^6.4.18",
    "tailwindcss": "^3.0.19",
    "typescript": "4.5.5"
  }
}
